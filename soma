section .data
msg db 20
var1 dw 0 
var2 dw 0 
firstDig dw 0
secondDig dw 0
result dw 0

section .text
global main
main:

    mov     ebp, esp; fot correct debugging
    mov     edx, 2; tamanho da area de memória
    mov     ecx, var1; carrega o endereço de memoria                                                                        
    mov     ebx, 0; file descriptor (stdin)
    mov     eax, 3; serviço de SO sys_read
    int     0x80; chamada de interrupção
    
    mov     eax, [var1]
    sub     eax, 30h
    mov     [var1], eax
    
    
    
    
    mov     ebp, esp; fot correct debugging
    mov     edx, 2; tamanho da area de memória
    mov     ecx, var2; carrega o endereço de memoria                                                                        
    mov     ebx, 0; file descriptor (stdin)
    mov     eax, 3; serviço de SO sys_read
    int     0x80; chamada de interrupção
    
    mov     eax, [var2]
    sub     eax, 30h
    mov     [var2], eax
    
    
    
    
    mov     eax, [var1]
    mov     ebx, [var2]
    add     eax, ebx
    add     eax, 30h
    mov     [result], eax
    
 
    mov     edx, 2;
    mov     ecx, result
    mov     ebx,1
    mov     eax,4
    int     0x80
    
    mov     eax,1
    int     0x80
    ret
    


